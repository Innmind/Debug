{
    "name": "innmind/debug",
    "type": "library",
    "description": "Set of tools to help debug applications",
    "keywords": ["debug", "http", "cli"],
    "homepage": "http://github.com/Innmind/Debug",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Debug/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/profiler": "^3.0.2",
        "innmind/framework": "~1.2",
        "innmind/http": "~6.3",
        "innmind/stack-trace": "~4.0",
        "innmind/object-graph": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Debug\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~5.6",
        "innmind/coding-standard": "~2.0"
    },
    "provide": {
        "innmind/framework-middlewares": "1.0"
    }
}
