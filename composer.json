{
    "name": "innmind/debug",
    "type": "library",
    "description": "Set of tools to help debug applications",
    "keywords": ["debug", "http", "cli"],
    "homepage": "http://github.com/Innmind/Debug",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Debug/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/rest-client": "~8.0",
        "innmind/http-framework": "~2.0",
        "innmind/stack-trace": "~3.0",
        "innmind/object-graph": "~2.0",
        "innmind/cli": "~2.0",
        "innmind/immutable": "~3.5",
        "innmind/operating-system": "~2.0",
        "innmind/json": "^1.1",
        "innmind/command-bus": "~4.0",
        "innmind/event-bus": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Debug\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Debug\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/coding-standard": "^1.1"
    }
}
