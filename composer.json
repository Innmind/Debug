{
    "name": "innmind/debug",
    "type": "library",
    "description": "Set of tools to help debug applications",
    "keywords": ["debug", "http", "cli"],
    "homepage": "http://github.com/Innmind/Debug",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Debug/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/rest-client": "^7.1",
        "innmind/time-continuum": "^1.3",
        "innmind/http-framework": "^1.3",
        "innmind/http": "~3.9",
        "innmind/stack-trace": "^2.0",
        "innmind/server-control": "^2.7",
        "innmind/object-graph": "^1.4",
        "innmind/cli": "^1.5",
        "innmind/immutable": "^2.13",
        "innmind/filesystem": "^3.3",
        "innmind/operating-system": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Debug\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Debug\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    }
}
